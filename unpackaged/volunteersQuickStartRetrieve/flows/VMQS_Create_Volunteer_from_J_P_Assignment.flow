                <elementReference>Update_PersonAccount</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_J_P_Assignment</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>New_Update_var_AcccountID</name>
        <label>Update var_AcccountID</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>var_AccountId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Upsert_PersonAccount</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_J_P_Assignment</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Update_PersonAccount_Fields</name>
        <label>Update PersonAccount Fields</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Get_PersonAccount.FirstName</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.First_Name__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>Get_PersonAccount.LastName</assignToReference>
