    <decisions>
        <name>Job_Positions_Returned</name>
        <label>More than 2 Job Positions Returned?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Count_Job_Position_Shifts</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>TwoPlusJobPositions</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_JobPositionCountFromVolunteerInitative</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>2.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Select_Job_Position</targetReference>
            </connector>
            <label>Greater Than or Equal to Two Job Positions</label>
        </rules>
        <rules>
            <name>OneJobPosition</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_JobPositionCountFromVolunteerInitative</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Single_Job_Position_from_Volunteer_Initiative</targetReference>
            </connector>
            <label>One Job Position</label>
        </rules>
        <rules>
            <name>Zero_Job_Positions</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_JobPositionCountFromVolunteerInitative</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_JP_Assignment_Fields_from_Volunteer_Initiative</targetReference>
            </connector>
            <label>Zero Job Positions</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determines the type of object that is being used for the inputID.</description>
        <name>Object_Reference</name>
        <label>What Object is Referenced in InputID?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_Volunteer_Initiative</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Volunteer Initiative</defaultConnectorLabel>
        <rules>
            <name>Job_Position</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>inputId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>0yi</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Job_Position</targetReference>
            </connector>
            <label>Job Position</label>
        </rules>
        <rules>
            <name>Job_Position_Shift</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>inputId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>1Qy</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Job_Position_Shift</targetReference>
            </connector>
            <label>Job Position Shift</label>
        </rules>
    </decisions>
