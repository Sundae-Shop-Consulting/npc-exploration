    <environments>Default</environments>
    <formulas>
        <name>formula_Email</name>
        <dataType>String</dataType>
        <expression>{!email}</expression>
    </formulas>
    <formulas>
        <name>formula_FirstName</name>
        <dataType>String</dataType>
        <expression>{!fName}</expression>
    </formulas>
    <formulas>
        <name>formula_JobPositionShift</name>
        <dataType>String</dataType>
        <expression>IF({!Job_Position},{!Get_Selected_JobPositionShift.Id},
IF({!Job_Position_Shift},{!Get_Job_Position_Shift.Id},
{!Get_Selected_JobPositionShift.Id}))</expression>
    </formulas>
    <formulas>
        <name>formula_JPAssignment_ActualDuration</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!Get_Selected_JobPositionShift.Id}),{!var_JPAssignment_ActualDuration},{!Get_Selected_JobPositionShift.Default_Duration__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>formula_JPAssignment_ScheduledEndTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT({!Get_Selected_JobPositionShift.EndDate})
+ &quot; &quot;
+ LEFT(TEXT({!Get_Selected_JobPositionShift.EndTime}),8))</expression>
    </formulas>
    <formulas>
        <name>formula_JPAssignment_ScheduledStartTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT({!Get_Selected_JobPositionShift.StartDate})
+ &quot; &quot;
+ LEFT(TEXT({!Get_Selected_JobPositionShift.StartTime}),8))</expression>
    </formulas>
    <formulas>
        <name>formula_LastName</name>
        <dataType>String</dataType>
        <expression>{!lName}</expression>
    </formulas>
